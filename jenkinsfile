

pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
                 echo "Hallo World"
            }
        }
        stage('Test') { 
            steps {
                 echo "Hallo Techstarter" 
            }
        }
        stage('Deploy') { 
            steps {
                 echo "Hallo Deutchland"
            }
        }
    }
}
 
