pipeline {
    agent {
        docker { 
            image 'maven:3.8.6-openjdk-18' }
        }    
    stages {
        stage('Build') {
            steps {
                sh 'mvn package '  
            }
        }
        stage('Test') {
            steps {
                sh 'mvn test'       
            }
        }
    }
}


